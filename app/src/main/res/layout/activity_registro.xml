<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.sebax.miapp.Registro">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/btnPresidenciales"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="Pesidenciales" />

        <Button
            android:id="@+id/btnDiputados"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Diputados"/>
        <Button
            android:id="@+id/btnSenadores"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Senadores"/>
        <Button
            android:id="@+id/btnCore"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Core"/>
    </LinearLayout>






</ScrollView>
